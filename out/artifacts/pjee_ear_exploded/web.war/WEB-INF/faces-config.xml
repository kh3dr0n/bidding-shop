<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <from-view-id>/inscription.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/produits.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


    <navigation-rule>
        <from-view-id>/inscription.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/inscription.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>



    <navigation-rule>
        <from-view-id>/produit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/produits.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>


    <navigation-rule>
        <from-view-id>/commande.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/produits.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>/admin/login.xhtml</from-view-id>
        <navigation-case>
            <to-view-id>/admin/categorie.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>